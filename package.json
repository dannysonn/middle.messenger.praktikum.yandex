{
  "name": "messanger",
  "engines": {
    "node": ">= 12"
  },
  "targets": {
    "modern": {
      "engines": {
        "browsers": ">= 50%"
      }
    }
  },
  "scripts": {
    "dev": "parcel src/index.html src/pages/**/*.html",
    "build": "parcel build src/index.html",
    "start": "node src/server.js",
    "sass": "sass src/styles/sass/main.scss src/styles/css/style.css",
    "lint": "eslint . --fix",
    "stylelint": "stylelint './src/styles/sass/*' --fix"
  },
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@anikin/parcel-transformer-handlebars-precompile": "1.0.1",
    "@parcel/transformer-sass": "2.3.2",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "parcel": "2.2.1",
    "stylelint": "^14.14.0",
    "stylelint-config-standard-scss": "^6.0.0",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "express": "4.18.1",
    "handlebars": "4.7.7",
    "nanoid": "^4.0.0",
    "stylelint-config-standard": "29.0.0",
    "stylelint-scss": "4.3.0"
  }
}
