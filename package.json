    "sass": "sass src/styles/sass/main.scss src/styles/css/style.css"
  },
  "devDependencies": {
    "parcel": "^2.7.0"
  },
  "dependencies": {
    "express": "^4.18.1",
    "handlebars": "^4.7.7"
  }
}
