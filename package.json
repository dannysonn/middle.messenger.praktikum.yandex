{
  "name": "messanger",
  "engines": {
    "node": ">= 12"
  },
  "targets": {
    "modern": {
      "engines": {
        "browsers": ">= 50%"
      }
    }
  },
  "scripts": {
    "dev": "parcel src/index.html src/pages/**/*.html",
    "build": "parcel build src/index.html",
